<article class='event-card event-item'>
  <img src='{{images.9.url}}' alt='' class='event-card__image' id='{{id}}' />
  <h3 class='event-card__title'>{{name}}</h3>
  <p class='event-card__date'>{{dates.start.localDate}}</p>
  {{#if _embedded.venues.0.name}}<a
      href='#'
      target='_blank'
      class='event-card__location'
    >{{_embedded.venues.0.name}}</a>{{/if}}
</article>