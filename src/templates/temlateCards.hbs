<li class='event-item' id='{{id}}'>
  <article class='event-card'>
    <img src='{{images.9.url}}' alt='' class='event-card__image' id='{{id}}' />
    <h3 class='event-card__title'>{{name}}</h3>
    <p class='event-card__date'>{{dates.start.localDate}}</p>
    <p class='event-card__location'>{{_embedded.venues.0.name}}</p>
  </article>
</li>